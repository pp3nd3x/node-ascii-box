  "devDependencies": {
    "@babel/cli": "7.0.0-beta.34",
    "@babel/core": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "@babel/preset-es2015": "7.0.0-beta.34",
    "@babel/preset-flow": "7.0.0-beta.34",
    "@babel/preset-react": "7.0.0-beta.34",
    "@babel/preset-stage-0": "7.0.0-beta.34",
    "@babel/preset-stage-1": "7.0.0-beta.34",
    "@babel/register": "7.0.0-beta.34",
    "babel-eslint": "8.0.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel7-jest": "1.0.1",
    "documentation": "6.3.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-eslint-plugin": "1.4.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
